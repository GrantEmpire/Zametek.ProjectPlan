    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetProjects.Extended.Wpf.Toolkit" Version="4.6.79" />
    <PackageReference Include="Zamatek.Windows.Interactivity.Core" Version="1.0.0" />
    <PackageReference Include="Zametek.Prism.AvalonDock.Core" Version="1.0.0" />
    <PackageReference Include="Zametek.Windows.AvalonDock.Core" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
